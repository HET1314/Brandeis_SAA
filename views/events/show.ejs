<h1>Event Data for <%= event.title %></h1>
<table>
  <tr>
    <th>Title</th>
    <td><%= event.title %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= event.description %></td>
  </tr>

  <tr>
    <th>Location</th>
    <td><%= event.location %></td>
  </tr>

  <tr>
    <th>Start Date</th>
    <td><%= event.startDate %></td>
  </tr>

  <tr>
    <th>End Date</th>
    <td><%= event.endDate %></td>
  </tr>

  <tr>
    <th>Is Online</th>
    <td><%= event.isOnline %></td>
  </tr>

  <tr>
    <th>Registration Link</th>
    <td><%= event.registrationLink %></td>
  </tr>

  <tr>
    <th>Organizer</th>
    <td><%= event.organizer %></td>
  </tr>

  <tr>
    <th>Attendees</th>
    <td><%= event.attendees %></td>
  </tr>

</table>
<% if (event.subscribedAccount) { %>
<h4 class="center">
  This user has a subscribed account</a
  >.
</h4>
<% } %>


<div>
  <a href="/events">View all events</a>
</div>
<div>
  <a href="/users/new">Interested but not user, click here to create an account! </a>
</div>